{
  "compilerOptions": {
      "outDir": "./dist",
      "sourceMap": false,
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "strictNullChecks": true,
      "module": "commonjs",
      "target": "es2015"
  },
  "include": [
      "./src/*.ts",
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}
